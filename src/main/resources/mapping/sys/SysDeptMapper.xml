    <select id="selectList" resultMap="BaseResultMap" parameterType="com.ciaj.boot.modules.sys.entity.vo.SysDeptVo">
        select
        <include refid="Base_Alias_Column_List"/>
        from sys_dept m
        <where>
            <trim prefix="" prefixOverrides="and" suffix="">
                <if test="id != null  and id != ''">
                    and m.id  =#{id}
                </if>
                <if test="name != null  and name != ''">
                    and m.name  like CONCAT('%',#{name},'%')
                </if>
                <if test="code != null  and code != ''">
                    and m.code  like CONCAT('%',#{code},'%')
                </if>
                <if test="areaId != null  and areaId != ''">
                    and m.area_id  =#{areaId}
                </if>
                <if test="level != null ">
                    and m.level = #{level}
                </if>
                <if test="type != null  and type != ''">
                    and m.type =#{type}
                </if>
                <if test="parentId != null  and parentId != ''">
                    and m.parent_id  =#{parentId}
                </if>
                <if test="parentIds != null  and parentIds != ''">
                    and m.parent_ids  like CONCAT('%',#{parentIds},'%')
                </if>
                <if test="enabled != null  and enabled != ''">
                    and m.enabled  =#{enabled}
                </if>

                <if test="delFlag != null  and delFlag != ''">
                    and m.del_flag = #{delFlag}
                </if>
                <!--  keyword   -->
                <if test="keyword != null and keyword != ''">
                    and m.name  like CONCAT('%',#{keyword},'%')
                </if>
            </trim>
        </where>
    </select>

    <select id="selectDTOList" resultMap="DResultMap" parameterType="com.ciaj.boot.modules.sys.entity.vo.SysDeptVo">
        select
        <include refid="Base_Alias_Column_List"/>,a.name as area_name,a.parent_ids as area_parent_ids
        from sys_dept m left  join sys_area a on m.area_id = a.id
        <where>
            <trim prefix="" prefixOverrides="and" suffix="">
                <if test="id != null  and id != ''">
                    and m.id  =#{id}
                </if>
                <if test="name != null  and name != ''">
                    and m.name  like CONCAT('%',#{name},'%')
                </if>
                <if test="code != null  and code != ''">
                    and m.code  like CONCAT('%',#{code},'%')
                </if>
                <if test="areaId != null  and areaId != ''">
                    and (m.area_id  =#{areaId} or a.parent_ids like CONCAT('%,',#{areaId},'%'))
                </if>
                <if test="level != null ">
                    and m.level = #{level}
                </if>
                <if test="type != null  and type != ''">
                    and m.type =#{type}
                </if>
                <if test="parentId != null  and parentId != ''">
                    and (m.id = #{parentId} or m.parent_id  =#{parentId} or m.parent_ids  like CONCAT('%,',#{parentId},'%'))
                </if>
                <if test="parentIds != null  and parentIds != ''">
                    and m.parent_ids  like CONCAT('%',#{parentIds},'%')
                </if>
                <if test="enabled != null  and enabled != ''">
                    and m.enabled  =#{enabled}
                </if>

                <if test="delFlag != null  and delFlag != ''">
                    and m.del_flag = #{delFlag}
                </if>
                <!--  keyword   -->
                <if test="keyword != null and keyword != ''">
                    and m.name  like CONCAT('%',#{keyword},'%')
                </if>
            </trim>
        </where>
